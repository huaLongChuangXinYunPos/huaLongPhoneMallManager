<?xml version="1.0" encoding="utf-8"?>
<manifest package="zhaoq.hl.hlphonemallmanager"
          xmlns:android="http://schemas.android.com/apk/res/android"
    >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        >
        <activity
            android:name=".LoginActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar"
            />
        <activity
            android:name=".tickets.TicketActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar"
            />
        <activity
            android:name=".brand.BrandInfoActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar"
            />
        <activity
            android:name=".goodsInfo.GoodsInfoActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar"
            />
        <activity
            android:name=".sellquery.SellQueryActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar"
            />
        <activity
            android:name=".tickets.activity.AddTicketInfoActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar"
            />
    </application>

</manifest>
